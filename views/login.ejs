<body>
   <div class="box" id = "Login-Div">
     <h2>Login</h2>
     <form class="LoginForm" method="POST">
        <div class="inputBox">
            <input type="text" name="userName" required="" id="UserName">
            <label>Username</label>
        </div>

        <div class="inputBox">
            <input type="password" name="password" required="" id="UserPassword">
            <label>Password</label>
        </div>
        <input type="submit"  name="" id="Submit" class="Submit" style="display: none" >
        <input type="button" name="" id="Log" value="Login">
        <input type="button" class="btn btn-warning" name="" id="Cancel" value="Cancel">
        <!-- <a href="#"><input type="button"  name="" value="SignUp" id ="SignUp"></a> -->
     </form>
     </div>
